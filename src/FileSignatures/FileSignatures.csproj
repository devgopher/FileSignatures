<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<Copyright>Neil Harvey</Copyright>
		<PackageProjectUrl>https://github.com/devgopher/botticelli</PackageProjectUrl>
		<RepositoryUrl>https://github.com/devgopher/botticelli</RepositoryUrl>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	</PropertyGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="OpenMcdf">
      <Version>2.3</Version>
    </PackageReference>
  </ItemGroup>

	<ItemGroup>
	  <PackageReference Include="OpenMcdf" Version="2.3.0" />
	</ItemGroup>
</Project>
